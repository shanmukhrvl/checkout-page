        display: flex;\r\n          gap: 1em;\r\n\r\n          &--sale {\r\n            color: #f2994a;\r\n          }\r\n\r\n          &--original {\r\n            text-decoration: line-through;\r\n            color: $light-black;\r\n          }\r\n        }\r\n      }\r\n\r\n      &__qty {\r\n        grid-area: item__qty;\r\n\r\n        display: inline-flex;\r\n        align-items: flex-end;\r\n\r\n        .controls {\r\n          display: flex;\r\n          padding: 1em 0.5em;\r\n          border: 1px solid $gray-3;\r\n          border-radius: 0.75rem;\r\n        }\r\n\r\n        input[type=\"number\"] {\r\n          -webkit-appearance: textfield;\r\n          -moz-appearance: textfield;\r\n          appearance: textfield;\r\n        }\r\n\r\n        input[type=\"number\"]::-webkit-inner-spin-button,\r\n        input[type=\"number\"]::-webkit-outer-spin-button {\r\n          -webkit-appearance: none;\r\n        }\r\n\r\n        button {\r\n          outline: none;\r\n          -webkit-appearance: none;\r\n          background-color: $gray-5;\r\n          border: none;\r\n          display: flex;\r\n          align-items: center;\r\n          justify-content: center;\r\n          width: 1.2rem;\r\n          height: 1.2rem;\r\n          cursor: pointer;\r\n          margin: 0;\r\n          position: relative;\r\n          border-radius: 0.25rem;\r\n          color: #828282;\r\n        }\r\n\r\n        input[type=\"number\"] {\r\n          font-size: 1rem;\r\n          width: 2rem;\r\n          height: 1.2rem;\r\n          margin: 0 1.5em;\r\n          text-align: center;\r\n          border: none;\r\n          background: inherit;\r\n        }\r\n      }\r\n    }\r\n  }\r\n\r\n  &__value {\r\n    margin-top: 5.625rem;\r\n\r\n    .row {\r\n      display: flex;\r\n      gap: 1em;\r\n      justify-content: space-between;\r\n      border-top: 1px solid $gray-4;\r\n    }\r\n\r\n    &--ship {\r\n      padding: 0.5em 0;\r\n    }\r\n\r\n    &--total {\r\n      padding: 1em 0;\r\n    }\r\n  }\r\n}\r\n\r\n.form {\r\n  display: flex;\r\n  flex-direction: column;\r\n\r\n  section {\r\n    margin-top: 2em;\r\n\r\n    h2 {\r\n      color: $gray-1;\r\n      margin-bottom: 0.75rem;\r\n    }\r\n  }\r\n\r\n  &__list {\r\n    margin: 0;\r\n    padding: 0;\r\n    list-style: none;\r\n\r\n    &--item {\r\n      margin-top: 1em;\r\n      display: flex;\r\n      flex-direction: column;\r\n\r\n      label {\r\n        color: $gray-2;\r\n      }\r\n\r\n      .inputWithIcon {\r\n        display: flex;\r\n        align-items: center;\r\n\r\n        .material-icons {\r\n          font-size: 1rem;\r\n          position: absolute;\r\n          margin-top: 0.1em;\r\n          left: 1.5rem;\r\n          z-index: 1;\r\n        }\r\n\r\n        input,\r\n        select {\r\n          width: 100%;\r\n          position: relative;\r\n          margin-top: 0.5em;\r\n          padding: 0.75em 0.5em 0.75rem 2rem;\r\n          border: 1px solid $gray-3;\r\n          border-radius: 0.75rem;\r\n          font-size: 0.75rem;\r\n\r\n          &::placeholder {\r\n            color: $gray-3;\r\n          }\r\n        }\r\n\r\n        select:invalid {\r\n          color: $gray-3;\r\n        }\r\n      }\r\n    }\r\n  }\r\n\r\n  .check--save {\r\n    margin: 1em 0 1.3em 0;\r\n\r\n    .checkBox {\r\n      margin-left: 0;\r\n    }\r\n  }\r\n\r\n  .submit--btn {\r\n    width: 10.3em;\r\n    padding: 1em 0;\r\n    color: #ffffff;\r\n    background: $orange;\r\n    border: none;\r\n    border-radius: 0.75rem;\r\n    align-self: flex-end;\r\n  }\r\n}\r\n","scss\\_config.scss":"$gray-1: #333333;\r\n$gray-2: #4f4f4f;\r\n$gray-3: #828282;\r\n$gray-4: #bdbdbd;\r\n$gray-5: #e0e0e0;\r\n$gray-6: #f2f2f2;\r\n$orange: #f2994a;\r\n$light-black: #4e5150;\r\n"},"lineCount":null}},"error":null,"hash":"b6e77e535e837bc7798fa3e08fa8edc0","cacheData":{"env":{}}}